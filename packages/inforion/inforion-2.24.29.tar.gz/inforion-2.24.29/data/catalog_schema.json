{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$id": "http://schema.infor.com/json-schema/CSVSchema2.json",
    "title": "CSVSchema2",
    "type": "object",
    "dialect": {
        "separator": ",",
        "skipLines": 1,
        "headerLine": 1
    },
    "properties": {
        "ID": {
            "type": "integer",
            "x-position": 1,
            "maximum": 9
        },
        "FIRST_NAME": {
            "type": "string",
            "x-position": 2,
            "maxLength": 25
        },
        "LAST_NAME": {
            "type": "string",
            "x-position": 3,
            "maxLength": 25
        },
        "COUNTRY": {
            "type": "string",
            "x-position": 4,
            "maxLength": 25
        }
    }
}
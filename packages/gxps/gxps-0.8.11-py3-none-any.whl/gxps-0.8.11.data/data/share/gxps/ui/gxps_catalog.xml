<?xml version="1.0" encoding="UTF-8"?>

<glade-catalog 
    name="gxps_catalog"
    domain="glade-3"
    depends="gtk+"
    version="1.0"
    icon-prefix="gxps"
    library="gladepython">
    <init-function>glade_python_init</init-function>

    <glade-widget-classes>
    
        <glade-widget-class
            title="GXPS Application Window"
            name="GXPSAppWindow" 
            parent="GtkApplicationWindow" 
            generic-name="gxpsappwindow"/>
            
        <glade-widget-class
            title="GXPS Import Dialog"
            name="GXPSImportDialog" 
            parent="GtkFileChooserDialog" 
            generic-name="gxpsimportdialog"/>
            
        <glade-widget-class
            title="GXPS Open Project Dialog"
            name="GXPSOpenProjectDialog" 
            parent="GtkFileChooserDialog" 
            generic-name="gxpsopenprojectdialog"/>
            
        <glade-widget-class
            title="GXPS Merge Project Dialog"
            name="GXPSMergeProjectDialog" 
            parent="GtkFileChooserDialog" 
            generic-name="gxpsmergeprojectdialog"/>
            
        <glade-widget-class
            title="GXPS Save Project Dialog"
            name="GXPSSaveProjectDialog" 
            parent="GtkFileChooserDialog" 
            generic-name="gxpssaveprojectdialog"/>
            
        <glade-widget-class
            title="GXPS Export TXT Dialog"
            name="GXPSExportTXTDialog" 
            parent="GtkFileChooserDialog" 
            generic-name="gxpsexporttxtdialog"/>
            
        <glade-widget-class
            title="GXPS Export IMG Dialog"
            name="GXPSExportIMGDialog" 
            parent="GtkFileChooserDialog" 
            generic-name="gxpsexportimgdialog"/>
            
        <glade-widget-class
            title="GXPS Save Confirmation Dialog"
            name="GXPSSaveConfirmationDialog" 
            parent="GtkDialog" 
            generic-name="gxpssaveconfirmationdialog"/>
            
        <glade-widget-class
            title="GXPS About Dialog"
            name="GXPSAboutDialog" 
            parent="GtkAboutDialog" 
            generic-name="gxpsaboutdialog"/>
            
        <glade-widget-class
            title="GXPS Edit Spectrum Dialog"
            name="GXPSEditSpectrumDialog"
            parent="GtkDialog"
            generic-name="gxpseditspectrumdialog"/>
            
        <glade-widget-class
            title="GXPS Spectrum TreeStore" 
            name="GXPSSpectrumTreeStore" 
            parent="GtkTreeStore" 
            generic-name="gxpsspectrumtreestore"/>
            
        <glade-widget-class 
            title="GXPS Peak TreeStore"
            name="GXPSPeakTreeStore"
            parent="GtkTreeStore"
            generic-name="gxpspeaktreestore"/>
            
        <glade-widget-class
            title="GXPS Canvas"
            name="GXPSCanvas"
            parent="GtkDrawingArea"
            generic-name="gxpscanvas"/>
            
        <glade-widget-class
            title="GXPS Export Canvas"
            name="GXPSExportCanvas"
            parent="GtkDrawingArea"
            generic-name="gxpsexportcanvas"/>
            
        <glade-widget-class
            title="GXPS CanvasObjects"
            name="GXPSCanvasObjects"
            parent="GtkDrawingArea"
            generic-name="gxpscanvasobjects"/>
            
        <glade-widget-class
            title="MPL Canvas"
            name="MPLCanvas"
            parent="GtkDrawingArea"
            generic-name="mplcanvas"/>
            
        <glade-widget-class
            title="GXPS Plot Toolbar"
            name="GXPSPlotToolbar"
            parent="GtkToolbar"
            generic-name="gxpsplottoolbar"/>
    </glade-widget-classes>
</glade-catalog>

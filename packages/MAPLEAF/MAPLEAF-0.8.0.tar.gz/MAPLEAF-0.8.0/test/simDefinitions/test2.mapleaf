# MAPLEAF
# See SimDefinitionTemplate.mapleaf for file format info & description of all options


Rocket{
    class               Rocket
    position            (0 0 4) # m
    initialDirection    (0 0 1)
    velocity            (10 0 50) #m/s
    angularVelocity     (0 0 1) #rad/s

    Sustainer{
        class           Stage
        stageNumber      0 #First and only stage

        UpperBodyTubeMass{
            class           Mass
            mass            50 
            position        (0 0 0)
            cg              (0 0 0)
        }

        Motor{
            class           Motor
            path            test/motorDefinitions/test.txt
        }
    }
}
from client.pywx.login_gui import LoginGui, App
from client.uri.connection import PipeData, Connection

if __name__ == '__main__':
    app = App()
    lg = LoginGui(PipeData, Connection, None, -1, "", size=(659, 500))
    lg.cook_tray(app)
    lg.Show()
    app.MainLoop()
